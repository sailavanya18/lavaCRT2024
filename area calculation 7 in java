import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int  a,l,b;
        float r;
        a=s.nextInt();
        l=s.nextInt();
        b=s.nextInt();
        r=s.nextFloat();
        int sq=a*a;
        int d=l*b;
        float c=(float)3.14*r*r;
        System.out.println(sq);
        System.out.println(d);
        System.out.printf("%.2f",c);
    }
}

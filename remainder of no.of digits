import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        long n=s.nextLong();
        long c=0,t;
        t=n;
        while(n>0){
            n=n/10;
            c++;
        }
        long r=t%c;
        System.out.println(r);
    }
}

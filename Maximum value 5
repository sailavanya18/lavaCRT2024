#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int i,a[n];
    for(i=0;i<n;i++){
        scanf("%d\n",&a[i]);
    }
    int max=a[0];
    for(i=1;i<n;i++){
        if(max<a[i]){
            max=a[i];
        }
    }
    printf("%d",max);
    return 0;
}

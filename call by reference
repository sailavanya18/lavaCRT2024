#include <stdio.h>
#include<stdlib.h>
int swap(int *x,int *y){
    int t;
    t=*x;
    *x=*y;
    *y=t;
}
int main()
{
 int a,b;
 printf("enter two numbers:");
 scanf("%d%d",&a,&b);
 printf("values before swapping:\n%d\n%d\n",a,b);
 swap(&a,&b);
 printf("values after swaping:\n%d\n%d",a,b);


    return 0;
}
